# Example environment variables needed for both local development and CI/CD
# Copy this to .env for local development and fill in proper values
# For CI/CD, set these as GitHub Secrets

# Application Configuration
MODELS_DIR="/app/models/"
CAT_DOG_OTHER_CLASSIFIER="cat-dog-other-classifier"  # Note the dashes, not underscores
MODEL_SOURCE="s3"  # Options: "local", "s3"
API_VERSION="v1"

# AWS Configuration
S3_MODELS_BUCKET="your-s3-bucket-name"
AWS_REGION="us-east-1"  # Example region
AWS_ACCESS_KEY_ID="your-aws-access-key-id" 
AWS_SECRET_ACCESS_KEY="your-aws-secret-access-key"

# Logging Configuration (Optional)
LOG_LEVEL="INFO"
JSON_LOGS="false"
